      <text:p text:style-name="P12"><text:span text:style-name="T8"><text:s text:c="2"/></text:span><text:span text:style-name="T8">INSERT INTO DEPTTRACE VALUES</text:span></text:p>
      <text:p text:style-name="P12"><text:span text:style-name="T8"><text:s text:c="14"/></text:span><text:span text:style-name="T8">(’</text:span><text:span text:style-name="T8">UPDATE’,SYSDATE, :NEW.name,:NEW.budget,:NEW.city,</text:span></text:p>
      <text:p text:style-name="P12"><text:span text:style-name="T8"><text:s text:c="33"/></text:span><text:span text:style-name="T8">:OLD.name,:OLD.budget,:OLD.city);</text:span></text:p>
      <text:p text:style-name="P12"><text:span text:style-name="T9"><text:s text:c="2"/></text:span><text:span text:style-name="T9">SELECT SUM(budget) FROM </text:span><text:span text:style-name="T8">DEPARTMENT;</text:span></text:p>
